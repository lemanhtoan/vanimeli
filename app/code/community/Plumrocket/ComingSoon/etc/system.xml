<?xml version="1.0"?>
<config>
    <tabs>
        <plumrocket>
            <label>
            <![CDATA[
                <div style="padding-left:22px; background: url(data:image/png;base64,) left 50% no-repeat;">Plumrocket Inc.</div> ]]>
            </label>
            <sort_order>101</sort_order>
        </plumrocket>
    </tabs>
    
    <sections>
        <comingsoon>
            <label>Coming Soon &amp; Maintenance Page</label>
            <tab>plumrocket</tab>
            <sort_order>1700</sort_order>
            <frontend_type>text</frontend_type>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            
            <groups>
                <general>
                    <label>General</label>
                    <expanded>1</expanded>
                    <sort_order>1</sort_order>
                    <frontend_type>text</frontend_type>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    
                    <fields>
                        <version translate="label">
                            <frontend_type>text</frontend_type>
                            <frontend_model>comingsoon/system_config_version</frontend_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </version>
                        <enable translate="label">
                            <label>Enable Extension</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>2</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </enable>
                        <serial translate="label">
                            <label>Serial Key</label>
                            <frontend_type>text</frontend_type>
                            <frontend_model>plumbase/system_config_form_field</frontend_model>
                            <sort_order>3</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>0</show_in_website>
                            <show_in_store>0</show_in_store>
                            <comment><![CDATA[
                                Your can find <strong>Serial Key</strong> in your account at <a target="_blank" href="https://store.plumrocket.com/downloadable/customer/products/">store.plumrocket.com</a>. For manual <a target="_blank" href="http://wiki.plumrocket.com/wiki/License_Installation">click here</a>.
                            ]]></comment>
                        </serial>
                        <ip_whitelist translate="label">
                            <label>IP Whitelist</label>
                            <frontend_type>textarea</frontend_type>
                            <sort_order>4</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>List of IP addresses that are allowed to bypass both coming soon and maintenance pages and access live site. This extension will be disabled for such visitors. Useful feature for website developers and administrators.</comment>
                        </ip_whitelist>
                        <ip_current translate="label">
                            <label>My Current IP</label>
                            <frontend_type>text</frontend_type>
                            <frontend_model>comingsoon/system_config_currentIp</frontend_model>
                            <sort_order>5</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </ip_current>

                    </fields>
                </general>

                <email>
                    <label>Email Templates</label>
                    <sort_order>2</sort_order>
                    <frontend_type>text</frontend_type>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>

                    <fields>
                        <welcome translate="label">
                            <label>Welcome Email</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_email_template</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </welcome>
                    </fields>
                </email>

                <mailchimp>
                    <label>Mailchimp Synchronization</label>
                    <sort_order>3</sort_order>
                    <frontend_type>text</frontend_type>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>

                    <fields>
                        <enable translate="label">
                            <label>Enable</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>If enabled, collected emails will be transferred to your Mailchimp List(s)</comment>
                        </enable>
                        <key translate="label">
                            <label>Mailchimp API Key</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>2</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>You can find API key in your Mailchimp account.</comment>
                            <depends>
                                <enable>1</enable>
                            </depends>
                        </key>
                        <list translate="label">
                            <label>Mailchimp Lists</label>
                            <frontend_type>multiselect</frontend_type>
                            <source_model>comingsoon/system_config_source_mailchimplist</source_model>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <can_be_empty/>
                            <comment>Collected emails will be saved in selected Mailchimp Email List(s)</comment>
                            <depends>
                                <enable>1</enable>
                            </depends>
                        </list>
                        <send_email translate="label">
                            <label>Double opt-in</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>40</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <can_be_empty/>
                            <comment>Important: Should be set to Yes. Abusing this may cause your account to be suspended. How does double opt-in work.</comment>
                            <depends>
                                <enable>1</enable>
                            </depends>
                        </send_email>
                        <info translate="label">
                            <label>Account Details</label>
                            <frontend_type>label</frontend_type>
                            <frontend_model>comingsoon/system_config_info</frontend_model>
                            <sort_order>50</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <depends>
                                <enable>1</enable>
                            </depends>
                        </info>
                    </fields>

                </mailchimp>

            </groups>
        </comingsoon>
    </sections>
</config>