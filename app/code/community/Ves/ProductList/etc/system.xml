<?xml version="1.0" encoding="UTF-8"?>
<!--
/**
 * Magento Product List extension
 *
 * @category   Ves
 * @package    Ves_ProductList
 * @author     venustheme.com
 */
 -->
<config>
  <tabs>
    <ves translate="label" module="productlist">
      <label>Venus Theme</label>
      <sort_order>211</sort_order>
    </ves>
  </tabs>
  <sections>
    <productlist module="productlist" translate="label">
      <label>Ves Product List</label>
      <tab>ves</tab>
      <frontend_type>text</frontend_type>
      <sort_order>10</sort_order>
      <show_in_default>1</show_in_default>
      <show_in_website>1</show_in_website>
      <show_in_store>1</show_in_store>
      <groups>
        <general_setting translate="label">
          <label>General Settings</label>
          <frontend_type>text</frontend_type>
          <sort_order>1000</sort_order>
          <show_in_default>1</show_in_default>
          <show_in_website>1</show_in_website>
          <show_in_store>1</show_in_store>
          <fields>
           <show translate="label" module="core">
             <label>Enabled</label>
             <sort_order>10</sort_order>
             <frontend_type>select</frontend_type>
             <source_model>adminhtml/system_config_source_yesno</source_model>
             <show_in_default>1</show_in_default>
             <show_in_website>1</show_in_website>
             <show_in_store>1</show_in_store>
           </show>
           <enable_owl translate="label" module="core">
             <label>Enabled OWL Carousel</label>
             <sort_order>20</sort_order>
             <frontend_type>select</frontend_type>
             <source_model>adminhtml/system_config_source_yesno</source_model>
             <show_in_default>1</show_in_default>
             <show_in_website>1</show_in_website>
             <show_in_store>1</show_in_store>
           </enable_owl>
           <enable_easytabs translate="label" module="core">
             <label>Enabled Easy Tabs</label>
             <sort_order>30</sort_order>
             <frontend_type>select</frontend_type>
             <source_model>adminhtml/system_config_source_yesno</source_model>
             <show_in_default>1</show_in_default>
             <show_in_website>1</show_in_website>
             <show_in_store>1</show_in_store>
           </enable_easytabs>
           <enable_bootstraptabs translate="label" module="core">
             <label>Enabled Bootstrap Tabs</label>
             <sort_order>40</sort_order>
             <frontend_type>select</frontend_type>
             <source_model>adminhtml/system_config_source_yesno</source_model>
             <show_in_default>1</show_in_default>
             <show_in_website>1</show_in_website>
             <show_in_store>1</show_in_store>
           </enable_bootstraptabs>
           <enable_countdown_js translate="label" module="core">
             <label>Enabled Countdown Deals</label>
             <sort_order>140</sort_order>
             <frontend_type>select</frontend_type>
             <source_model>adminhtml/system_config_source_yesno</source_model>
             <show_in_default>1</show_in_default>
             <show_in_website>1</show_in_website>
             <show_in_store>1</show_in_store>
           </enable_countdown_js>
           
         </fields>
       </general_setting>
       <deals_setting translate="label">
          <label>Deals Settings</label>
          <frontend_type>text</frontend_type>
          <sort_order>2000</sort_order>
          <show_in_default>1</show_in_default>
          <show_in_website>1</show_in_website>
          <show_in_store>1</show_in_store>
          <fields>
           <show_discount translate="label" module="core">
             <label>Show Discount Number</label>
             <frontend_type>select</frontend_type>
             <source_model>adminhtml/system_config_source_yesno</source_model>
             <sort_order>10</sort_order>
             <show_in_default>1</show_in_default>
             <show_in_website>1</show_in_website>
             <show_in_store>1</show_in_store>
           </show_discount>
           <show_save_price translate="label" module="core">
             <label>Show Save Price</label>
             <frontend_type>select</frontend_type>
             <source_model>adminhtml/system_config_source_yesno</source_model>
             <sort_order>20</sort_order>
             <show_in_default>1</show_in_default>
             <show_in_website>1</show_in_website>
             <show_in_store>1</show_in_store>
           </show_save_price>
           <show_number_bought translate="label" module="core">
             <label>Show Number Bought</label>
             <frontend_type>select</frontend_type>
             <source_model>adminhtml/system_config_source_yesno</source_model>
             <sort_order>30</sort_order>
             <show_in_default>1</show_in_default>
             <show_in_website>1</show_in_website>
             <show_in_store>1</show_in_store>
           </show_number_bought>
           <show_countdown_timer translate="label" module="core">
             <label>Show Countdown Timer</label>
             <frontend_type>select</frontend_type>
             <source_model>adminhtml/system_config_source_yesno</source_model>
             <sort_order>40</sort_order>
             <show_in_default>1</show_in_default>
             <show_in_website>1</show_in_website>
             <show_in_store>1</show_in_store>
           </show_countdown_timer>
           
         </fields>
       </deals_setting>
     </groups>
   </productlist>
 </sections>
</config>